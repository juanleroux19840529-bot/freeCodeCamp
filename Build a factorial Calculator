** start of script.js **

   let number=3
  let num = number;
 

function factorialCalculator (num){

  let result = 1;
 
  for (let i = 1; i<= num; i++){
    result *= i 
  }
  
  return result
  
 
}
factorialCalculator(num);
const factorial = (factorialCalculator(num));
const resultMsg = `Factorial of ${num} is ${factorial}`;
console.log(resultMsg)




** end of script.js **
