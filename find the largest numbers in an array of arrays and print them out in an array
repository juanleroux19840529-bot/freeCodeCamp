function largestOfAll(arrayOfArrays){
  let arrayOfLargstNumbers = [];
for (const innerArray of arrayOfArrays){
 const largstNumber = Math.max(...innerArray)
  arrayOfLargstNumbers.push(largstNumber)
}

return arrayOfLargstNumbers
}

console.log(largestOfAll([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]])) 
