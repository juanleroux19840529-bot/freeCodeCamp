function addTogether(num1,num2  ){
  // Step 1: Check if 'a' is not a number
  if (typeof num1 !== "number") return undefined;


if(arguments.length > 1){
  // Check if 'b' is a valid number
  if( typeof num2 !=="number")return undefined;
 
 // Return their sum if both are valid numbers
  return num1 + num2
 
 }
 //if (num2 === undefined) return undefined

 // Step 3: If only one argument (a) is provided,
  // return a new function waiting for the second argument

return function(second){
   // Validate that the second argument is a number
  if (typeof second !== "number") return undefined;

    // Return the sum of 'a' and the second argument
    return num1 + second;
  }
}



console.log(addTogether(1,7)) 
console.log(addTogether(5, undefined))
console.log(addTogether(5)) 
console.log(addTogether(5, undefined))
console.log(addTogether(5)(5))
