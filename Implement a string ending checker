** start of script.js **

function confirmEnding(str,target){
  console.log(str);
  console.log(target)
  let ending = str.substring(str.length - target.length);
  console.log(ending)
  if(ending===target){
    return true;
  }else{
    return false
  }
}


console.log(confirmEnding("Hello", "l"))

** end of script.js **
